{
  "version": 2,
  "builds": [
    {
      "src": "api/hello.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/hello",
      "dest": "/api/hello.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/hello.py"
    }
  ],
  "functions": {
    "api/hello.py": {
      "maxDuration": 10
    }
  }
}
